{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "dev-debug",
      "displayName": "Dev Debug",
      "description": "picobench development config. Debug build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "PICOBENCH_BUILD_TOOLS": "ON",
        "PICOBENCH_BUILD_TESTS": "ON",
        "PICOBENCH_BUILD_EXAMPLES": "ON"
      }
    }
  ]
}